# override the default project variables. ----------------------------------------------------------
# aws region.
#aws_region = "us-east-2"

# desired number of eks worker nodes.
#aws_eks_desired_node_count = 3

# aws eks node group instance type.
#aws_eks_instance_type = ["t3a.xlarge"]

# disk size for aws eks node instances.
#aws_eks_instance_disk_size = 160

# kubernetes version to use for the eks cluster.
# valid aws eks versions are: 1.24, 1.25, 1.26, 1.27, 1.28, and 1.29.
#aws_eks_kubernetes_version = "1.29"

# name of ssh public key for eks worker nodes.
#ssh_pub_key_name = "<your_ssh_pub_key_name_here>"

# cisco cloud observability kubernetes and app service monitoring solution helm chart variables.
#cco_client_id = "<your_cco_client_id_here>"
#cco_client_secret = "<your_cco_client_secret_here>"
#cco_cluster_name = "<your_cco_cluster_name_here>"
#cco_collector_endpoint = "<your_cco_collector_endpoint_here>"
#cco_operators_endpoint = "<your_cco_operators_endpoint_here>"
#cco_tenant_id = "<your_cco_tenant_id_here>"
#cco_token_url = "<your_cco_token_url_here>"

# cisco cloud observability security monitoring helm chart variables.
#cco_security_monitoring_enabled = false
#cco_agent_id = "<your_cco_agent_id_here>"
#cco_shared_secret = "your_cco_shared_secret_here>"
